<h1>Add a music venue</h1>

<form action="/venues" method="POST">
<!-- p*5>label+input -->
<p>
  <label for="">Name:</label>
  <input type="text" name="venue[name]">
</p>
<p>
  <label for="">Location:</label>
  <input type="text" name="venue[location]">
</p>
<p>
  <label for="">Capacity:</label>
  <input type="number" name="venue[capacity]">
</p>
<p>
  <label for="">Image:</label>
  <input type="url" name="venue[image]">
</p>
<input type="submit" value="Add Venue">

</form>
